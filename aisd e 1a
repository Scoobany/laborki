int main()
{
    int tab[6] = { 1,123,89,3,4,7 };

    for (int i = 0; i < 6; i++)
    {
        if (tab[i] > tab[i + 1])
        {
            swap(tab[i], tab[i + 1]);
        };
    }
    for (int i = 0; i < 6; i++)
    {
        if (tab[i] < tab[i + 1])
        {
            swap(tab[i], tab[i + 1]);

        };
    };
    for (int i = 0; i < 6; i++)
    {
        cout << tab[i] << endl;
    }
}
